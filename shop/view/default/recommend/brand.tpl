<script>
    $(document).ready(function($) {
        $('#pet_brand_{$code} .our_partners').owlCarousel({
            items: 6,
            responsive: {
                0 : {
                    items: 2
                },
                480 : {
                    items: 3
                },
                768 : {
                    items: 4
                },
                992 : {
                    items: 6
                }
            },
            loop: true,
            dots: false,
            nav: false,
            slideSpeed: 400,
            autoplayHoverPause: true,
            autoPlay: 4000,
            addClassActive: true,
            onInitialized: callback
        });
        function callback(event) {
            if (this._items.length > this.options.items) {
                $('#pet_brand_{$code} .navslider').show();
            } else {
                $('#pet_brand_{$code} .navslider').hide();
            }
        }
        $('#pet_brand_{$code} .navslider .prev').on('click',
        function(e) {
            e.preventDefault();
            $('#pet_brand_{$code} .our_partners').trigger('prev.owl.carousel');
        });
        $('#pet_brand_{$code} .navslider .next').on('click',
        function(e) {
            e.preventDefault();
            $('#pet_brand_{$code} .our_partners').trigger('next.owl.carousel');
        });
    });
</script>