            var ajax_url="http://demo.snstheme.com/sns-avaz/index.php/producttabs/index/ajaxslider";
            var catids = '3,33,43,45';
            var label_allready =  'All ready';

            var options = '{"align_title":"justify","effects":"slideRight","isenabled":"1","list_products_by":"category","number_per_display":"12","number_per_loadmore":"4","order_type":"created_at","product_category":"3,33,43,45","product_order_by":"is_featured","product_order_dir":"asc","screen_lg":"4","screen_md":"4","screen_sm":"3","screen_xs":"2","show_number_loadmore":"0","styleTitle":"center","title":"New Products"}';

            itemNav.click(function(){
                var $this = $(this);
                if($this.hasClass('tab-nav-actived active')) return false;
                itemNav.removeClass('tab-nav-actived active');
                $this.addClass('tab-nav-actived active');
                var itemActive = '.'+$this.attr('data-href');
                itemContent.removeClass('tab-content-actived').css({'overflow':'hidden', 'height':'0'});
                $(itemActive, $element).addClass('tab-content-actived').css({'overflow':'', 'height':''});

                contentLoading = $('.content-loading',$(itemActive, $element));
                isLoaded = $(itemActive, $element).hasClass('is-loaded');
                if(!isLoaded && !$(itemActive, $element).hasClass('is-loading')){
                    $(itemActive, $element).addClass('is-loading');
                    contentLoading.show();
                    pdt_type = $this.attr('data-type');
                    catid = $this.attr('data-catid');
                    orderby = $this.attr('data-orderby');
                    $.ajax({
                        type: 'POST',
                        url: ajax_url,
                        data:{
                            numberstart: 0,
                            catid: catid,
                            orderby: orderby,
                            catids: catids,
                            options: options,
                            pdt_type: pdt_type
                        },
                        success: function(result){
                            $this.addClass('tab-nav-actived active');
                            if(result.listProducts !=''){
                                $('.pdt-list',$(itemActive, $element)).html(result.listProducts);
                                $(itemActive, $element).addClass('is-loaded').removeClass('is-loading');
                            }
                        },
                        dataType:'json'
                    });
                }
            });
            $(window).load(function(){
                itemNav.each(function(){
                    if($(this).hasClass('first_load')) $(this).trigger('click');
                });
            });
    })('#pet_new_product_{$code}');
});
